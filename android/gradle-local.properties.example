# Copy this file to android/gradle-local.properties (do not commit) and fill with your keystore values
# Path can be absolute or relative to android/app
MYAPP_UPLOAD_STORE_FILE=app/release.keystore
MYAPP_UPLOAD_STORE_PASSWORD=your-store-password
MYAPP_UPLOAD_KEY_ALIAS=your-key-alias
MYAPP_UPLOAD_KEY_PASSWORD=your-key-password

# Alternatively, set environment variables instead of local properties:
# ANDROID_KEYSTORE=/absolute/path/to/release.keystore
# ANDROID_KEYSTORE_PASSWORD=your-store-password
# ANDROID_KEY_ALIAS=your-key-alias
# ANDROID_KEY_PASSWORD=your-key-password
